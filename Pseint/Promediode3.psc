Algoritmo Promediode3
	Definir a Como Entero
	Definir c Como Real
	A <- 1
	Mientras A <= 3
		Imprimir "Alumno ", A
		Definir SC Como Real
		SC <- 0
		a <- 1
		Mientras a <= 5 Hacer
			Imprimir "Calf ", a , ": "
			Leer c
			SC <- SC + c
			a <- a + 1
		FinMientras
		Imprimir "Promedio del Alumno ", A , ": ", SC / 5
		A <- A +1
	FinMientras
	
FinAlgoritmo
